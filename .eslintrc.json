// Use this file as a starting point for your project's .eslintrc.
// Copy this file, and add rule overrides as needed.
{
	"extends": "airbnb",
	"rules": {
		"no-var": 1,
		"consistent-return": 0,
		"no-trailing-spaces": 0,
		"strict": 0,
		"no-underscore-dangle": 0,
		// Things below this line should be resolved
		"import/newline-after-import": 1,
		"indent": [
			"warn",
			2
		],
		"no-useless-escape": 0,
		"no-console": 0,
		"no-nested-ternary": 0,
		"prefer-destructuring": 0,
		"no-dupe-keys": 0,
		"max-len": 0,
		"curly": 1,
		"spaced-comment": 1,
		"func-names": 0,
		"prefer-arrow-callback": 0,
		"object-shorthand": 0,
		"comma-dangle": 1,
		"no-shadow": 1,
		"no-useless-concat": 1,
		"padded-blocks": 1,
		"no-param-reassign": 0,
		"brace-style": 1,
		"prefer-template": 1,
		"no-unused-vars": 1,
		"no-else-return": 1,
		"object-curly-spacing": 1,
		"eqeqeq": 1,
		"vars-on-top": 1,
		"one-var": 1,
		"global-require": 1,
		"camelcase": 1,
		"no-use-before-define": 1,
		"block-scoped-var": 1,
		"radix": 1,
		"no-plusplus": 1,
		"no-mixed-operators": 1,
		"no-mixed-spaces-and-tabs": 1,
		"no-tabs": 1,
		"no-redeclare": 1,
		"require-yield": 1,
		"no-undef": 1,
		"no-lonely-if": 1,
		"no-throw-literal": 1,
		"no-loop-func": 1,
		"new-cap": 1,
		"guard-for-in": 1,
		"no-restricted-syntax": 0,
		"no-unused-expressions": 1,
		"no-sequences": 1,
		"no-empty": 1,
		"no-unreachable": 1,
		"no-multi-spaces": 1,
		"no-bitwise": 1,
		"no-continue": 1,
		"arrow-body-style": [
			"error",
			"always"
		]
	}
}